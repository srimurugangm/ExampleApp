    <access origin="*"/>
    <allow-navigation href="*"/>
<preference name="zumoUrl" value="http://localhost:4000"/><preference name="zumoAuthenticationMethod" value=""/><preference name="zumoUseBlobStorage" value=""/><plugin name="com.commontime.cordova.public.messaging" spec="0.0.31"/></widget>