    <plugin name="cordova-plugin-statusbar" spec="1.0.1"/>
    <plugin name="cordova-plugin-file-opener2" spec="2.0.2"/>
    <plugin name="cordova-plugin-camera" spec="2.2.0"/>

    <plugin name="com.telerik.plugins.healthkit" spec="0.4.2"/>
    <plugin name="phonegap-plugin-barcodescanner" spec="5.0.1"/>
    <plugin name="cordova-universal-clipboard" spec="0.1.0"/>

